{
  "name": "markdown-cleaner-cli",
  "version": "0.1.5",
  "description": "A fast, zero-config CLI tool to clean and format Markdown files.",
  "main": "src/index.js",
  "bin": {
    "mdclean": "src/index.js"
  },
  "scripts": {
    "test": "jest",
    "start": "node ./src/index.js",
    "format": "prettier --write \"**/*.{js,md,yml,yaml}\"",
    "docs:tree": "node src/utils/generate-tree.js",
    "docs:toc": "doctoc README.md",
    "ready": "npm run docs:tree && npm run docs:toc && npm run format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ioncakephper/markdown-cleaner-cli.git"
  },
  "keywords": [
    "markdown",
    "cli",
    "cleaner",
    "formatter",
    "linter",
    "optimizer",
    "tool",
    "utility",
    "node",
    "nodejs",
    "md",
    "documentation",
    "text-processing"
  ],
  "homepage": "https://github.com/ioncakephper/markdown-cleaner-cli",
  "files": [
    "src/",
    "LICENSE",
    "README.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md"
  ],
  "author": "ioncakephper",
  "license": "MIT",
  "devDependencies": {
    "doctoc": "^2.2.1",
    "doctrine": "^3.0.0",
    "espree": "^10.1.0",
    "esprima": "^4.0.1",
    "jest": "^30.0.5",
    "npm-registry-fetch": "^16.0.0",
    "prettier": "^3.3.2"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "glob": "^11.0.3"
  }
}
